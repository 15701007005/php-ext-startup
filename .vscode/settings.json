{
    "files.associations": {
        "php.h": "c",
        "php_ini.h": "c",
        "zend.h": "c",
        "type_traits": "c",
        "typeinfo": "cpp",
        "stdarg.h": "c",
        "common.h": "c",
        "config.h": "c",
        "stringy.h": "c"
    },
    "files.autoSave": "onFocusChange",
    "editor.fontSize": 18,
    "editor.renderWhitespace": "boundary",
    "editor.cursorStyle": "line-thin",
    "files.exclude": {
      "**/.git": true,
      "**/.svn": true,
      "**/.hg": true,
      "**/CVS": true,
      "**/.DS_Store": true
    }
}
